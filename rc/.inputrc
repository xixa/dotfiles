#set editing-mode vi

set expand-tilde on
set completion-ignore-case on
set page-completions on
set show-all-if-ambiguous on
# set show-all-if-unmodified off
set convert-meta off
set input-meta on
set output-meta on
set visible-stats on
set mark-symlinked-directories on
set visible-stats on
# set -o vi


#Meta-Rubout: backward-kill-word
Tab: menu-complete
# shift+tab shows all option (show-all-if-ambiguous is enabled)
"\e[Z": complete
"\C-l": clear-screen
# # "\C-u": kill-whole-line
# "\e[1~": beginning-of-line
# "\e[4~": end-of-line
# "\e[5~": history-search-backward
# "\e[6~": history-search-forward
# "\e[3~": delete-char
# "\e[2~": quoted-insert
# "\e[5C": forward-word
# "\e[5D": backward-word
# "\e\e[C": forward-word
# "\e\e[D": backward-word
