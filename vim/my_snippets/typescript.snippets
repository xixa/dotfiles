extends javascript

snippet imp "Creates import" b
import ${2:{${3:...}}} from '${1:lib}'
endsnippet

snippet pi "ignore prettier"
// prettier-ignore
endsnippet

snippet log "console.log shorthand"
console.log($0)
endsnippet

snippet err "console.err shorthand"
console.error($0)
endsnippet

snippet doc "creates doc"
/**
 * $0
 */
endsnippet

snippet bc "Block comment"
/* $1 */
endsnippet

snippet sw "Switch block"
switch (${1}) {
	case ${2}:
		$0
	default:
		break
}
endsnippet
